{
  "name": "better-markdown",
  "version": "1.0.0",
  "description": "Extended markdown-it plugins for Obsidian-style wikilinks, checkboxes, callouts, and more",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:lib": "rollup -c",
    "dev": "vite",
    "demo": "vite",
    "preview": "vite preview",
    "prepublishOnly": "npm run build:lib"
  },
  "keywords": [
    "markdown",
    "markdown-it",
    "wikilinks",
    "obsidian",
    "checkboxes",
    "callouts",
    "editor",
    "codemirror",
    "monaco",
    "footnotes",
    "block-ids"
  ],
  "author": "uvinduachinthya",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/uvinduachinthya/better-markdown.git"
  },
  "bugs": {
    "url": "https://github.com/uvinduachinthya/better-markdown/issues"
  },
  "homepage": "https://github.com/uvinduachinthya/better-markdown#readme",
  "peerDependencies": {
    "markdown-it": "^14.0.0"
  },
  "dependencies": {
    "markdown-it": "^14.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@vitejs/plugin-react": "^4.3.4",
    "dompurify": "^3.2.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup": "^4.28.1",
    "vite": "^6.0.5"
  }
}